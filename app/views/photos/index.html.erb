<%= simple_form_for @new_photo do |f| %>
  <div class="large-6 columns">
    <%= f.input :image, type: :file, :input_html => { :multiple => true, class: "large-4 columns " }, label: false %></div>
<%= f.button :submit, 'Map them!' %>
<% end %>
<div id = "my_map" class = "enable_map">

  <div class='map_tool photo_container '>
    <div id='mapped_photos_container' class='photo_group disable_map'>Mapped</div>
    <div id='unmapped_photos_container' class='photo_group disable_map'>Unmapped</div>
  </div>

  <div class="map_tool" id="showUnplaced" class="ui-widget-content"> See Unplaced Photos </div>
</div>



